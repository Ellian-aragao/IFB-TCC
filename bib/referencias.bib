@article{Magalhães_Filho_Marcheri_2022, title={O enfrentamento jurídico dos ataques de Ransomware no Brasil e no mundo: The legal confrontation of Ransomware attacks in Brazil and in the world}, volume={8}, url={https://ojs.brazilianjournals.com.br/ojs/index.php/BRJD/article/view/51922}, DOI={10.34117/bjdv8n9-102}, abstractNote={&amp;lt;p&amp;gt;A presente pesquisa tem como foco investigar se a postura adotada pela Brasil em comparação com a postura adotada pelo estado de Wyoming (EUA) e por Portugal é eficiente em termos de legislação, prevenção e coação a prática da extorsão digital, decorrentes de ataques de &amp;lt;em&amp;gt;ransomware, &amp;lt;/em&amp;gt;poisgeram diversas implicações práticas e problemas a toda sociedade, através de prejuízos políticos, econômicos e sociais. O ataque tem por finalidade pedir um resgaste dos dados, que geralmente é exigido em criptomoeda, que não está regulada pelo Estado brasileiro, o que dificulta o rastreio até o agente, ocasionando na sua impunidade. Visando a pesquisa científica nesse tocante, surgiu o interesse pelo tema resultando a realização deste trabalho. Quanto as opções metodológicas, trata-se de pesquisa jurídico-sociológica e jurídico-propositivo do tipo interpretativo com a utilização de método indutivo e dedutivo e o emprego de fontes diretas e indiretas por meio de análise de conteúdo.&amp;lt;/p&amp;gt;}, number={9}, journal={Brazilian Journal of Development}, author={Magalhães, Gabriela Alves Aires and Filho, Raphael Hernandes Parra and Marcheri, Pedro Lima}, year={2022}, month={Sep.}, pages={61971–61984} }

